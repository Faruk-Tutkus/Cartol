{
  "expo": {
    "name": "Cartol",
    "slug": "Cartol",
    "version": "1.0.0",
    "orientation": "portrait",
    "backgroundColor": "#4A4947",
    "icon": "./assets/images/icon.png",
    "scheme": "Cartol",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "newArchEnabled": true,
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      "expo-image-picker"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
